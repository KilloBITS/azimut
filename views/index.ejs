			<a href="/about">О нас</a> 
			<a href="/news">Новости</a> 
			<a href="#">VHF</a> 
			<a href="#" class="menu_to_left">HF</a> 
			<a href="#" class="menu_to_right">Форум</a> 
			<a href="#">Барахолка</a> 
			<a href="#">Контакты</a> 
		</nav>
	</header>
	<main class="wrapper">
		<div class="nav_menu left">
			<div class="nav_wrapper">
				<div class="wrapper_title">Навигация</div>
				<div class="pre_line"></div>
				<a href="#" id="activity">Мероприятия</a>
				<a href="#" id="calendar">Календарь</a>
				<a href="#" id="Numeral">Цифра</a>
				<a href="#" id="Communities">Сообщества</a>
				<a href="#" id="Round_tables">Круглые столы</a>
				<a href="#" id="market">Барахолка</a>
				<a href="#" id="miscellanea">Разное</a>
				<a href="#" id="Technical_section">Технический раздел</a>
				
			</div>
		</div>
		<div class="nav_menu right">
			<% if(sessionUser !== undefined){ %>
			<div class="nav_wrapper" id="users_wraper">
				<div class="wrapper_title">Пользователь</div>
				<div class="pre_line"></div>
				<div class="nav_loader"></div>
				<div class="userAVA" style="background-image: url(../../../data/avatars/default.gif)"></div>
				<div class="userDATA">
					<div class="userLogin"><%= sessionUser %></div>
					<a href="/getProfile" class="userButton">Профиль</a>
				</div>
				<a href="/logout" class="userlogoutInBlock" title="Выход">Выход</a>		
			</div>
			<% } else { %>
			<div class="nav_wrapper">
				<div class="wrapper_title">Авторизация</div>
				<div class="pre_line"></div>
				<div class="nav_loader"></div>
				<input type="text" name="emailRadio" id="emailRadio" class="authdata" placeholder="Логин">
				<input type="password" name="passwordRadio" id="passwordRadio" class="authdata" placeholder="Пароль">
				<input type="button" value="Авторизация" class="auth_btn" id="signIn" onclick="signIn()">
				<a href="/register" class="NewPassLink">Регистрация</a>
				<a href="#" class="NewPassLink">Забыли пароль?</a>
			</div>
			<% } %>   
			
			
			<div class="nav_wrapper">
				<div class="wrapper_title">Информация</div>
				<div class="pre_line"></div>
				<div class="info_div">Действующие репитеры:</div>
				<div class="ii_div">Тест репитер 1</div>
				<div class="ii_div">Тест репитер 2</div>
				<div class="ii_div">Тест репитер 3</div>
				<div class="ii_div">Тест репитер 4</div>
				<div class="info_div">Действующие частоты:</div>
				<div class="ii_div">27.1100</div>
				<div class="ii_div">37.7750</div>
				<div class="ii_div">42.7500</div>
				<div class="ii_div">42.9875</div>
				<div class="ii_div">145.2000</div>
				<div class="ii_div">145.3500</div>
				<div class="ii_div">145.4250</div>
				<div class="ii_div">148.4250</div>
				<div class="info_div">Круглые столы (Львов):</div>
				<div class="ii_div">Тест стол</div>
				<div class="ii_div">Рыцари</div>
				<div class="ii_div">MicroGIS</div>
				<div class="ii_div">Phalcon</div>
			</div>

			<div class="nav_wrapper">
				<div class="wrapper_title">APRS</div>
				<div class="pre_line"></div>
				<a class="none_text" href="https://www.aprsdirect.com/center/49.82786,24.03119/zoom/11/time/60" target="_blank">
					<img src="../../../img/aprs.png" width="228">
					<div class="a_btn">Открыть карту</div>
				</a>
			</div>
		</div>
		<% for(var i = 0; i < NEWS.length; i++){ %>
		<section id="section_<%= i %>" class="section_data">
			<h2><a class="title_news_link" href="/detailsnews&ai=<%= i %>"><%= NEWS[i].title %></a></h2>
			<p>
				<img class="alignleft size-thumbnail wp-image-622" src="<%= NEWS[i].NEWS_LOGO %>" alt="" width="150" height="104">
				<%= NEWS[i].TEXT %>	
				<div class="shadow_transparent"></div>
			</p>
			<!-- <% for(var ii = 0; ii < NEWS[i].CONTENT_IMAGE.length; ii++){ %>
			<img src="<%= NEWS[i].CONTENT_IMAGE[ii] %>">
			<% } %> -->
			
			<div class="section_date">
				<%= NEWS[i].DATE %>
				<span><%= NEWS[i].AUTHOR %></span>
			</div>
		</section>	
		<% } %>
